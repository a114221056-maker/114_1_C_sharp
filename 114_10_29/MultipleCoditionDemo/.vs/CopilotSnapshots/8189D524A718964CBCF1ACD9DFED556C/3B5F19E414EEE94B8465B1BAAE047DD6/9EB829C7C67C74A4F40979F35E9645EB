namespace MultipleCoditionDemo
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void btnJudge_Click(object sender, System.EventArgs e)
        {
            string input = txtScore.Text.Trim();
            System.Windows.Forms.MessageBox.Show($"輸入的成績: {input}", "等級判斷");
        }
    }
}
